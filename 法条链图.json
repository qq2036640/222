{
  "案由": "诈骗罪判定与量刑任务",
  "版本": "1.0",
  "生效时间": "2022-03-01",
  "法律依据": {
    "法律名称": "中华人民共和国刑法",
    "条款": "第266条",
    "原文": "《中华人民共和国刑法》第二百六十六条【诈骗罪】诈骗公私财物，数额较大的，处三年以下有期徒刑、拘役或者管制，并处或者单处罚金；数额巨大或者有其他严重情节的，处三年以上十年以下有期徒刑，并处罚金；数额特别巨大或者有其他特别严重情节的，处十年以上有期徒刑或者无期徒刑，并处罚金或者没收财产。本法另有规定的，依照规定。"
  },
  "节点列表": [
    {
      "id": "node_001",
      "名称": "案情导入",
      "说明": "接收案件基础信息，包括案情描述、案发时间、案发地点等",
      "类型": "输入节点",
      "输入字段": ["案情描述", "案发时间", "案发地点"],
      "输出字段": ["案情文本"]
    },
    {
      "id": "node_002",
      "名称": "嫌疑人识别",
      "说明": "从案情中识别所有涉案嫌疑人及其人数",
      "类型": "提取节点",
      "输入字段": ["案情文本"],
      "输出字段": ["嫌疑人列表", "嫌疑人人数"],
      "法律依据": {
        "法律名称": "中华人民共和国刑法",
        "条款": "第25条",
        "原文": "共同犯罪是指二人以上共同故意犯罪。"
      }
    },
    {
      "id": "node_003",
      "名称": "主犯从犯判定",
      "说明": "判定各嫌疑人在共同犯罪中的角色，区分主犯和从犯",
      "类型": "判定节点",
      "输入字段": ["嫌疑人列表", "案情文本"],
      "输出字段": ["主犯列表", "从犯列表"],
      "法律依据": {
        "法律名称": "中华人民共和国刑法",
        "条款": "第26条、第27条",
        "原文": "《中华人民共和国刑法》第二十六条【主犯】组织、领导犯罪集团进行犯罪活动的或者在共同犯罪中起主要作用的，是主犯。第二十七条【从犯】在共同犯罪中起次要或者辅助作用的，是从犯。对于从犯，应当从轻、减轻处罚或者免除处罚。"
      }
    },
    {
      "id": "node_004",
      "名称": "主体要件审查",
      "说明": "审查嫌疑人是否达到刑事责任年龄且具有刑事责任能力",
      "类型": "判定节点",
      "输入字段": ["嫌疑人列表"],
      "输出字段": ["主体适格结果"],
      "法律依据": {
        "法律名称": "中华人民共和国刑法",
        "条款": "第17条、第18条",
        "原文": "已满十六周岁的人犯罪，应当负刑事责任。精神病人在不能辨认或者不能控制自己行为的时候造成危害结果，经法定程序鉴定确认的，不负刑事责任。"
      }
    },
    {
      "id": "node_005",
      "名称": "客体要件审查",
      "说明": "审查犯罪行为是否侵犯公私财产所有权",
      "类型": "判定节点",
      "输入字段": ["案情文本"],
      "输出字段": ["客体侵犯结果"],
      "法律依据": {
        "法律名称": "中华人民共和国刑法",
        "条款": "第266条",
        "原文": "诈骗罪侵犯的客体是公私财产所有权。"
      }
    },
    {
      "id": "node_006",
      "名称": "主观要件审查",
      "说明": "审查嫌疑人是否具有非法占有目的和欺诈故意",
      "类型": "判定节点",
      "输入字段": ["案情文本", "嫌疑人列表"],
      "输出字段": ["主观故意结果"],
      "法律依据": {
        "法律名称": "中华人民共和国刑法",
        "条款": "第266条",
        "原文": "诈骗罪的主观方面表现为故意，并且具有非法占有公私财物的目的。"
      }
    },
    {
      "id": "node_007",
      "名称": "客观要件审查",
      "说明": "审查是否存在虚构事实、隐瞒真相的欺诈行为",
      "类型": "判定节点",
      "输入字段": ["案情文本"],
      "输出字段": ["客观行为结果"],
      "法律依据": {
        "法律名称": "中华人民共和国刑法",
        "条款": "第266条",
        "原文": "诈骗罪客观方面表现为使用欺诈方法骗取数额较大的公私财物。"
      }
    },
    {
      "id": "node_008",
      "名称": "数额情节评估",
      "说明": "根据涉案金额和案发地区判定数额档次和情节严重程度",
      "类型": "计算节点",
      "输入字段": ["涉案金额", "案发地区"],
      "输出字段": ["数额档次", "情节严重度"],
      "规则": {
        "浙江_数额较大": 6000,
        "浙江_数额巨大": 100000,
        "浙江_数额特别巨大": 500000,
        "北京_数额较大": 5000,
        "北京_数额巨大": 100000,
        "北京_数额特别巨大": 500000,
        "全国默认_数额较大": 3000,
        "全国默认_数额巨大": 30000,
        "全国默认_数额特别巨大": 500000
      },
      "法律依据": {
        "法律名称": "最高人民法院、最高人民检察院关于办理诈骗刑事案件具体应用法律若干问题的解释",
        "条款": "法释〔2011〕7号第一条",
        "原文": "诈骗公私财物价值三千元至一万元以上、三万元至十万元以上、五十万元以上的，应当分别认定为刑法第二百六十六条规定的\"数额较大\"\"数额巨大\"\"数额特别巨大\"。各省、自治区、直辖市高级人民法院、人民检察院可以结合本地区经济社会发展状况，在前款规定的数额幅度内，共同研究确定本地区执行的具体数额标准，报最高人民法院、最高人民检察院备案。"
      }
    },
    {
      "id": "node_009",
      "名称": "基准刑计算",
      "说明": "根据数额档次确定基准刑期",
      "类型": "计算节点",
      "输入字段": ["数额档次", "涉案金额"],
      "输出字段": ["基准刑期_月"],
      "规则": {
        "数额较大_基准刑_最小": 6,
        "数额较大_基准刑_最大": 18,
        "数额巨大_基准刑_最小": 36,
        "数额巨大_基准刑_最大": 48,
        "数额特别巨大_基准刑_最小": 120,
        "数额特别巨大_基准刑_最大": 144
      },
      "法律依据": {
        "法律名称": "最高人民法院关于常见犯罪的量刑指导意见",
        "条款": "法发〔2021〕21号",
        "原文": "构成诈骗罪的，根据下列情形在相应的幅度内确定量刑起点：（1）达到数额较大起点的，在一年以下有期徒刑、拘役幅度内确定量刑起点。（2）达到数额巨大起点或者有其他严重情节的，在三年至四年有期徒刑幅度内确定量刑起点。（3）达到数额特别巨大起点或者有其他特别严重情节的，在十年至十二年有期徒刑幅度内确定量刑起点。依法应当判处无期徒刑的除外。"
      }
    },
    {
      "id": "node_010",
      "名称": "从犯调节",
      "说明": "对从犯适用量刑调节系数",
      "类型": "计算节点",
      "输入字段": ["角色", "基准刑期_月"],
      "输出字段": ["从犯调节系数"],
      "规则": {
        "从犯调节系数_最小": -0.5,
        "从犯调节系数_最大": -0.2
      },
      "法律依据": {
        "法律名称": "中华人民共和国刑法",
        "条款": "第27条",
        "原文": "《中华人民共和国刑法》第二十七条【从犯】在共同犯罪中起次要或者辅助作用的，是从犯。对于从犯，应当从轻、减轻处罚或者免除处罚。"
      }
    },
    {
      "id": "node_011",
      "名称": "自首坦白调节",
      "说明": "评估自首或坦白情节并计算调节系数",
      "类型": "计算节点",
      "输入字段": ["案情文本"],
      "输出字段": ["自首坦白调节系数"],
      "规则": {
        "自首调节系数_最小": -0.4,
        "自首调节系数_最大": 0,
        "坦白调节系数_最小": -0.2,
        "坦白调节系数_最大": 0
      },
      "法律依据": {
        "法律名称": "中华人民共和国刑法",
        "条款": "第67条",
        "原文": "《中华人民共和国刑法》第六十七条【自首】犯罪以后自动投案，如实供述自己的罪行的，是自首。对于自首的犯罪分子，可以从轻或者减轻处罚。其中，犯罪较轻的，可以免除处罚。犯罪嫌疑人虽不具有前两款规定的自首情节，但是如实供述自己罪行的，可以从轻处罚；因其如实供述自己罪行，避免特别严重后果发生的，可以减轻处罚。"
      }
    },
    {
      "id": "node_012",
      "名称": "退赃退赔调节",
      "说明": "评估退赃退赔情况并计算调节系数",
      "类型": "计算节点",
      "输入字段": ["案情文本", "涉案金额"],
      "输出字段": ["退赃退赔调节系数"],
      "规则": {
        "退赃比例30以下_调节系数": -0.1,
        "退赃比例30至70_调节系数_最小": -0.1,
        "退赃比例30至70_调节系数_最大": -0.2,
        "退赃比例70以上_调节系数_最小": -0.2,
        "退赃比例70以上_调节系数_最大": -0.3
      },
      "法律依据": {
        "法律名称": "最高人民法院关于常见犯罪的量刑指导意见",
        "条款": "法发〔2021〕21号",
        "原文": "对于退赃、退赔的，综合考虑犯罪性质，退赃、退赔行为对损害结果所能弥补的程度，退赃、退赔的数额及主动程度等情况，可以减少基准刑的30%以下。"
      }
    },
    {
      "id": "node_013",
      "名称": "累犯调节",
      "说明": "判断是否构成累犯并计算调节系数",
      "类型": "判定节点",
      "输入字段": ["案情文本"],
      "输出字段": ["累犯调节系数"],
      "规则": {
        "累犯调节系数_最小": 0.1,
        "累犯调节系数_最大": 0.4
      },
      "法律依据": {
        "法律名称": "中华人民共和国刑法",
        "条款": "第65条",
        "原文": "《中华人民共和国刑法》第六十五条【一般累犯】被判处有期徒刑以上刑罚的犯罪分子，刑罚执行完毕或者赦免以后，在五年以内再犯应当判处有期徒刑以上刑罚之罪的，是累犯，应当从重处罚，但是过失犯罪和不满十八周岁的人犯罪的除外。"
      }
    },
    {
      "id": "node_014",
      "名称": "最终刑期计算",
      "说明": "综合基准刑和各项调节系数计算最终刑期",
      "类型": "计算节点",
      "输入字段": ["基准刑期_月", "从犯调节系数", "自首坦白调节系数", "退赃退赔调节系数", "累犯调节系数"],
      "输出字段": ["最终刑期_月", "最终刑期_文本"]
    },
    {
      "id": "node_015",
      "名称": "缓刑审查",
      "说明": "审查是否符合缓刑适用条件",
      "类型": "判定节点",
      "输入字段": ["最终刑期_月", "累犯调节系数", "案情文本"],
      "输出字段": ["是否适用缓刑", "缓刑考验期_月"],
      "规则": {
        "缓刑条件_最高刑期_月": 36,
        "缓刑考验期_拘役_最小": 2,
        "缓刑考验期_拘役_最大": 12,
        "缓刑考验期_有期_最小": 12,
        "缓刑考验期_有期_最大": 60
      },
      "法律依据": {
        "法律名称": "中华人民共和国刑法",
        "条款": "第72条、第73条、第74条",
        "原文": "《中华人民共和国刑法》第七十二条【适用条件】对于被判处拘役、三年以下有期徒刑的犯罪分子，同时符合下列条件的，可以宣告缓刑，对其中不满十八周岁的人、怀孕的妇女和已满七十五周岁的人，应当宣告缓刑：（一）犯罪情节较轻；（二）有悔罪表现；（三）没有再犯罪的危险；（四）宣告缓刑对所居住社区没有重大不良影响。第七十四条【累犯不适用缓刑】对于累犯和犯罪集团的首要分子，不适用缓刑。"
      }
    },
    {
      "id": "node_016",
      "名称": "量刑建议汇总",
      "说明": "汇总所有量刑要素形成量刑建议",
      "类型": "汇总节点",
      "输入字段": ["最终刑期_月", "是否适用缓刑", "缓刑考验期_月"],
      "输出字段": ["量刑建议"]
    },
    {
      "id": "node_017",
      "名称": "结构化输出",
      "说明": "生成结构化的判决建议文本",
      "类型": "输出节点",
      "输入字段": ["嫌疑人列表", "主犯列表", "从犯列表", "量刑建议"],
      "输出字段": ["判决建议文本"]
    }
  ],
