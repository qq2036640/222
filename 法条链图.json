{
  "图结构说明": "中国刑法诈骗罪要件判定流程图，用于自动解析案情并输出量刑建议",
  "版本": "1.0",
  "创建日期": "2026-02-08",
  "适用法律": "中华人民共和国刑法第266条（诈骗罪）",
  "节点": [
    {
      "id": "start",
      "名称": "案情输入",
      "类型": "输入节点",
      "说明": "输入案件案情文本，包含嫌疑人信息、案件事实等"
    },
    {
      "id": "extract_suspects",
      "名称": "提取嫌疑人信息",
      "类型": "提取节点",
      "说明": "从案情文本中提取嫌疑人列表、人数、基本信息",
      "输出字段": ["嫌疑人列表", "嫌疑人人数"]
    },
    {
      "id": "extract_roles",
      "名称": "提取角色信息",
      "类型": "提取节点",
      "说明": "判定每名嫌疑人的角色：主犯、从犯或胁从犯",
      "输出字段": ["主犯列表", "从犯列表", "胁从犯列表"]
    },
    {
      "id": "check_subject",
      "名称": "检查犯罪主体",
      "类型": "要件判定节点",
      "说明": "判断嫌疑人是否具备刑事责任能力（已满16周岁，具有完全刑事责任能力的自然人或单位）",
      "法律依据": "刑法第17条、第30条",
      "判定条件": ["年龄>=16周岁", "具有刑事责任能力"]
    },
    {
      "id": "check_object",
      "名称": "检查犯罪客体",
      "类型": "要件判定节点",
      "说明": "判断是否侵犯了公私财产所有权",
      "法律依据": "刑法第266条",
      "判定条件": ["侵犯财产权利", "财产损失存在"]
    },
    {
      "id": "check_subjective",
      "名称": "检查主观方面",
      "类型": "要件判定节点",
      "说明": "判断嫌疑人是否具有非法占有目的，主观上明知",
      "法律依据": "刑法第266条，最高法司法解释",
      "判定条件": ["非法占有目的", "故意心态", "明知是诈骗"]
    },
    {
      "id": "check_objective",
      "名称": "检查客观方面",
      "类型": "要件判定节点",
      "说明": "判断是否实施了虚构事实、隐瞒真相的欺骗行为，被害人基于错误认识处分财产",
      "法律依据": "刑法第266条",
      "判定条件": ["虚构事实或隐瞒真相", "被害人产生错误认识", "被害人处分财产", "嫌疑人获取财产", "被害人遭受损失"]
    },
    {
      "id": "check_amount",
      "名称": "检查诈骗数额",
      "类型": "要件判定节点",
      "说明": "判断诈骗数额是否达到刑事立案标准",
      "法律依据": "最高人民法院、最高人民检察院《关于办理诈骗刑事案件具体应用法律若干问题的解释》",
      "判定条件": {
        "数额较大": "3000元-30000元（各地标准不同）",
        "数额巨大": "30000元-500000元",
        "数额特别巨大": ">=500000元"
      }
    },
    {
      "id": "check_circumstances",
      "名称": "检查其他严重情节",
      "类型": "要件判定节点",
      "说明": "检查是否存在加重或减轻情节",
      "加重情节": ["电信网络诈骗", "针对特殊群体", "跨境诈骗", "诈骗集团", "累犯", "多次诈骗"],
      "减轻情节": ["自首", "坦白", "立功", "退赃退赔", "从犯", "未遂", "中止"]
    },
    {
      "id": "determine_crime",
      "名称": "犯罪构成判定",
      "类型": "判定节点",
      "说明": "综合四要件判定是否构成诈骗罪",
      "判定逻辑": "主体适格 AND 客体侵犯 AND 主观故意 AND 客观行为存在 AND (数额达标 OR 情节严重)"
    },
    {
      "id": "calculate_base_sentence",
      "名称": "计算基准刑",
      "类型": "量刑计算节点",
      "说明": "根据诈骗数额计算基准刑期",
      "量刑标准": {
        "数额较大": "三年以下有期徒刑、拘役或者管制，并处或者单处罚金",
        "数额巨大或有其他严重情节": "三年以上十年以下有期徒刑，并处罚金",
        "数额特别巨大或有其他特别严重情节": "十年以上有期徒刑或者无期徒刑，并处罚金或者没收财产"
      }
    },
    {
      "id": "adjust_for_circumstances",
      "名称": "情节调整",
      "类型": "量刑调整节点",
      "说明": "根据加重减轻情节调整刑期",
      "调整规则": {
        "自首": "可以从轻或者减轻处罚",
        "立功": "可以从轻或者减轻处罚",
        "从犯": "应当从轻、减轻处罚或者免除处罚",
        "累犯": "应当从重处罚",
        "退赃退赔": "可以从轻处罚",
        "电信网络诈骗": "酌情从重处罚"
      }
    },
    {
      "id": "adjust_for_role",
      "名称": "角色调整",
      "类型": "量刑调整节点",
      "说明": "根据主从犯角色调整量刑",
      "调整规则": {
        "主犯": "按照其所参与的或者组织、指挥的全部犯罪处罚",
        "从犯": "应当从轻、减轻处罚或者免除处罚",
        "胁从犯": "应当按照其犯罪情节减轻处罚或者免除处罚"
      }
    },
    {
      "id": "calculate_final_sentence",
      "名称": "确定最终刑期",
      "类型": "量刑输出节点",
      "说明": "综合基准刑和各项调整确定最终量刑建议",
      "输出字段": ["刑期下限", "刑期上限", "罚金建议", "缓刑可能性"]
    },
    {
      "id": "generate_output",
      "名称": "生成输出报告",
      "类型": "输出节点",
      "说明": "生成完整的分析报告",
      "输出内容": [
        "嫌疑人列表及人数",
        "各嫌疑人角色判定",
        "构成要件判定结果",
        "各嫌疑人量刑建议",
        "法律依据说明"
      ]
    }
  ],
  "边": [
    {
      "id": "edge_1",
      "source": "start",
      "target": "extract_suspects",
      "条件说明": "开始处理案情"
    },
    {
      "id": "edge_2",
      "source": "extract_suspects",
      "target": "extract_roles",
      "条件说明": "提取到嫌疑人信息后识别角色"
    },
    {
      "id": "edge_3",
      "source": "extract_roles",
      "target": "check_subject",
      "条件说明": "识别角色后逐人检查要件"
    },
    {
      "id": "edge_4",
      "source": "check_subject",
      "target": "check_object",
      "条件说明": "主体适格则检查客体"
    },
    {
      "id": "edge_5",
      "source": "check_object",
      "target": "check_subjective",
      "条件说明": "客体侵犯则检查主观方面"
    },
    {
      "id": "edge_6",
      "source": "check_subjective",
      "target": "check_objective",
      "条件说明": "主观故意则检查客观行为"
    },
    {
      "id": "edge_7",
      "source": "check_objective",
      "target": "check_amount",
      "条件说明": "客观行为存在则检查数额"
    },
    {
      "id": "edge_8",
      "source": "check_amount",
      "target": "check_circumstances",
      "条件说明": "数额检查后查看其他情节"
    },
    {
      "id": "edge_9",
      "source": "check_circumstances",
      "target": "determine_crime",
      "条件说明": "综合判定是否构成犯罪"
    },
    {
      "id": "edge_10",
      "source": "determine_crime",
      "target": "calculate_base_sentence",
      "条件说明": "构成犯罪则计算基准刑"
    },
    {
      "id": "edge_11",
      "source": "determine_crime",
      "target": "generate_output",
      "条件说明": "不构成犯罪则直接输出"
    },
    {
      "id": "edge_12",
      "source": "calculate_base_sentence",
      "target": "adjust_for_circumstances",
      "条件说明": "基准刑确定后进行情节调整"
    },
    {
      "id": "edge_13",
      "source": "adjust_for_circumstances",
      "target": "adjust_for_role",
      "条件说明": "情节调整后进行角色调整"
    },
    {
      "id": "edge_14",
      "source": "adjust_for_role",
      "target": "calculate_final_sentence",
      "条件说明": "完成所有调整后确定最终刑期"
    },
    {
      "id": "edge_15",
      "source": "calculate_final_sentence",
      "target": "generate_output",
      "条件说明": "量刑确定后生成输出报告"
    }
  ],
  "验证规则": {
    "节点验证": "所有边引用的节点ID必须在节点列表中存在",
    "自环检查": "所有边的source和target必须不同",
    "连通性": "从start节点应能到达所有关键节点"
  }
}
